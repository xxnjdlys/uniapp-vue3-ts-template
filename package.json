    "@dcloudio/uni-mp-weixin": "3.0.0-3061520221228001",
    "@dcloudio/uni-quickapp-webview": "3.0.0-3061520221228001",
    "@dcloudio/uni-ui": "^1.4.22",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "await-to-js": "^3.0.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.6",
    "lodash-es": "^4.17.21",
    "luch-request": "^3.0.8",
    "pinia": "^2.0.25",
    "qs": "^6.11.0",
    "vue": "^3.2.45",
    "vue-demi": "^0.13.11",
    "vue-i18n": "^9.2.2"
